{{#each this}}
<div class="col-lg-4 col-sm-6">
  <div class="thumbnail">
    <h3>{{name}}</h3>
    <p>Age: {{age}}</p>
    <p>Gender: {{sex}}</p>
    <h4> Skills</h4>
    <ol>
    {{#each skills}}
      <li>{{this}}</li>
    {{/each}}
    </ol>
    <div>
      <button id="editFamilyMember" type="button" class="btn btn-primary">Edit Family Member</button>
      </div>
    <button id="deleteFamilyMember" type="button" class="btn btn-danger">Delete Family Member</button>
  </div>
</div>
{{/each}}


